	file = {Chandler_Pugh_2021_Islands and the rise of correlational epistemology in the Anthropocene.pdf:/Users/erinrobinson/Zotero/storage/WEWHQWMW/Chandler_Pugh_2021_Islands and the rise of correlational epistemology in the Anthropocene.pdf:application/pdf},
