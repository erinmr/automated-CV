	doi = {10.1007/978-3-030-37177-7_7},
	pages = {121--143},
	file = {Springer Full Text PDF:/Users/erinrobinson/Zotero/storage/9X7YTCL5/Bechtel - 2020 - Data Journeys Beyond Databases in Systems Biology.pdf:application/pdf},
}

