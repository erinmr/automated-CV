	file = {IEEE Xplore Abstract Record:/Users/erinrobinson/Zotero/storage/9RIVF47X/8781923.html:text/html;Wofford et al_2020_Jupyter Notebooks as Discovery Mechanisms for Open Science_annotated.pdf:/Users/erinrobinson/Zotero/storage/CVJQAWNZ/Wofford et al_2020_Jupyter Notebooks as Discovery Mechanisms for Open Science_annotated.pdf:application/pdf;Wofford et al_2020_Jupyter Notebooks as Discovery Mechanisms for Open Science.pdf:/Users/erinrobinson/Zotero/storage/CVJQAWNZ/Wofford et al_2020_Jupyter Notebooks as Discovery Mechanisms for Open Science.pdf:application/pdf},
