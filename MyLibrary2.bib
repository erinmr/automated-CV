@article{chandler_islands_2021,
	title = {Islands and the rise of correlational epistemology in the {Anthropocene}: {Rethinking} the trope of the {\textquoteleft}canary in the coalmine{\textquoteright}},
	volume = {16},
	issn = {17152593},
	shorttitle = {Islands and the rise of correlational epistemology in the {Anthropocene}},
	url = {https://islandstudiesjournal.org/files/ISJChandlerPughCorrelationalEpistemologyAnthropocene.pdf},
	doi = {10.24043/isj.119},
	abstract = {Once on the periphery of international debate, today small islands are seen by many as key to unlocking new ways of thinking about climate change and developing new practices of adaptation in the epoch of the Anthropocene. These approaches differ starkly from modernist, linear, causal frameworks that construct islands as vulnerable objects that require {\textquoteleft}saving{\textquoteright} or {\textquoteleft}protecting{\textquoteright}. Instead, islands become instruments of productive knowledge, laboratories for investigation and learning, fundamental to an alternative, correlational, epistemology. In analysing these approaches, we take the prolific trope of islands as the {\textquoteleft}canaries in the coalmine{\textquoteright} in order to draw out the ontological implications of instrumentalising islands as {\textquoteleft}correlational machines{\textquoteright} in the Anthropocene. We raise fundamental problems with this literal instrumentalisation of islands and islanders, drawing out how these logics reduce island life to merely sensing and attuning to the co-relational entanglements of the Anthropocene, rather than offering higher normative aspirations for political change.},
	language = {en},
	number = {1},
	urldate = {2022-01-28},
	journal = {Island Studies Journal},
	author = {Chandler, David and Pugh, Jonathan},
	month = may,
	year = {2021},
	keywords = {\_tablet},
